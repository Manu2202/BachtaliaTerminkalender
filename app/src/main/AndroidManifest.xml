<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.bachtalia.bachtaliaterminkalenderv3">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Bachtalia Terminkalender"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Launcher">
        <activity android:name=".LoginAndMain.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginAndMain.MainActivity" />
        <activity android:name=".Events.ListAndDetails.EventsListActivity" />
        <activity android:name=".PackinglistDatabase.PackingList" />
        <activity android:name=".Events.ListAndDetails.EventDetailsActivity" />
        <activity android:name=".Contact.ContactsActivity" />

    </application>

</manifest>